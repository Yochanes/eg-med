<div class="tab-pane" id="latestp">
<div class="row rless">
  <div id="latest" class="owl-theme owl-carousel">
{% set temp = 0 %}
{% set setCol = 2  %}
 {% for product in products %}

  {% set temp = temp + 1 %}

  {% if temp % setCol == 1 %}
      <div class="multi-row">
  {% endif %}
  <div class="product-layout col-xs-12 cless">
    <div class="product-thumb transition">
      <div class="image"><a href="{{ product.href }}"><img src="{{ product.thumb }}" alt="{{ product.name }}" title="{{ product.name }}" class="img-responsive center-block" /></a>
        <!-- Webiarch Images Start -->
                        {% set t = 0 %}
                          {% for webi_image in product.webi_images %}
                          {% if t == 0 %}
                           <a href="{{ product.href }}"><img src="{{ webi_image.popup }}" class="img-responsive second-img" alt="hover image"/></a>
                          {% set t = t + 1 %}
                          {% endif %}
                        {% endfor %}
        <!-- Webiarch Images End -->
       {% if product.price %}
          {% if product.special %}
               <span class="sale">{{ pro_sale }}</span>
          {% endif %}
      {% endif %} 

          {% if product.rating %}
          <div class="rating text-center">
            {% for i in 1..5 %}
            {% if product.rating < i %}
            <span class="fa fa-stack">
              <i class="fa fa-star-o fa-stack-2x"></i>
            </span>
            {% else %}
            <span class="fa fa-stack">
              <i class="fa fa-star fa-stack-2x"></i><i class="fa fa-star-o fa-stack-2x"></i>
            </span>
            {% endif %}
          {% endfor %}</div>{% else %}
          <div class="rating text-center">{% for i in 1..5 %}
          <span class="fa fa-stack"><i class="fa fa-star-o fa-stack-2x"></i></span>
          {% endfor %}</div>
        {% endif %}

      </div>
      <div class="caption text-center">
         
        <div class="opbtn">
        <h4  class="protitle"><a href="{{ product.href }}">{{ product.name }}</a></h4>
        
         {% if product.price %}
        <div class="price">
          {% if not product.special %}
          {{ product.price }}
          {% else %}
          <span class="price-new">{{ product.special }}</span> <span class="price-old">{{ product.price }}</span>
          {% endif %}
         {#  {% if product.tax %}
          <span class="price-tax">{{ text_tax }} {{ product.tax }}</span>
          {% endif %} #}
        </div>
        {% endif %}
      </div>
       
<div class="button-group">
        

        <div class="absbtn">
           <button type="button" data-toggle="tooltip" title="{{ button_cart }}" onclick="cart.add('{{ product.product_id }}');" class="cartb"><svg><use xlink:href="#pcart"></use></svg><span><!-- {{ button_cart }} --></span></button>
          <div data-toggle="tooltip" title="Quickview"  class="bquickv"></div>
         <div class="wish"><button type="button" data-toggle="tooltip" title="{{ button_wishlist }}" onclick="wishlist.add('{{ product.product_id }}');" class="bwish"><svg width="21px" height="20px"><use xlink:href="#heart"></use></svg><span><!-- {{ button_wishlist }} --></span></button></div>
         <button class="compare" type="button" data-toggle="tooltip" title="{{ button_compare }}" onclick="compare.add('{{ product.product_id }}');"><svg><use xlink:href="#compare"/></svg></button>

         </div>
      </div>
      </div>
      
    </div>
  </div>

    {% if temp % setCol == 0 %}
            </div>
        {% endif %}
    {% endfor %}

  {% if products|length % setCol != 0 %}
    <!-- <h1>hii</h1> -->
        </div>
    {% endif %}
      </div>
    </div>

</div>

</div>

<script type="text/javascript">
   (function($){
    $("#latest").owlCarousel({
    itemsCustom : [
   [0, 1],
    [320, 2],
    [500, 3],
    [767, 3],
    [900, 4],
    [1200, 4],
    [1410, 4]
    ],
      // autoPlay: 1000,
       navigationText: ['<svg width="18px" height="18px"><use xlink:href="#larrow"></use></svg><span>PREV</span>', '<span>NEXT</span><svg width="18px" height="18px"><use xlink:href="#rarrow"></use></svg>'],
      navigation : true,
      pagination: false
    });
     }(jQuery));
</script>
