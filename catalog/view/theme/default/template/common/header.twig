<body>
  <header>
    <div class="hsticky">
        <div class="hbottom">
          <div class="col-xs-2 col-sm-1 hidden-md hidden-lg hidden-xl appmenu"></div>
             <div id="logo" class="col-lg-2 col-md-2 col-sm-6 col-xs-5">

                 {% if logo %}
                     <a href="{{ home }}">
                         <img src="{{ logo }}" title="{{ name }}" alt="{{ name }}" class="img-responsive logo" />
                     </a>
                 {% else %}
                    <h1><a href="{{ home }}">{{ name }}</a></h1>
                 {% endif %}</div>

               <div class="col-lg-7 col-md-7 col-xs-12 menusp">
                   <ul class="nav navbar-nav">
                       <li><a class="popup-with-form" href="#popupmenu">Магазин</a></li>
                       <li><a href="/index.php?route=information/information&information_id=4">О бренде</a></li>
                       <li><a href="#">{{ text_menu_informations }}</a></li>
                       <li><a href="{{ contact }}">{{ text_menu_contact }}</a></li>
                   </ul>
               </div>

            <div id="top-links" class="col-lg-3 col-md-3 col-xs-5 col-sm-5 text-right">
                <div class="wr_top_right">
{#                    <a class="popup-with-form" href="#search">Поиск</a>#}
                    <ul class="list-inline">
                        {% if false %}
                            <li class="dropdown insetting">
                                <a href="setting" title="setting" class="dropdown-toggle" data-toggle="dropdown">
                                    <svg class="" width="28px" height="28px"><use xlink:href="#hsetting"></use></svg>
                                    <span>{{ text_setting }}</span>
                                </a>
                                <ul class="dropdown-menu dropdown-menu-right hsetting">
                                    <li class="langcur">{{ currency }}</li>
                                    <li class="langcur">{{ language }}</li>
                                </ul>
                            </li>
                        {% endif %}
                        <li class="mycart">{{ cart }}</li>

                        <li class="wish-li"><a href="{{ wishlist }}" id="wishlist-total" title="{{ text_wishlist }}"><svg><use xlink:href="#heart"></use></svg><span><!-- {{ text_wishlist }} --></span></a></li>

                        <li class="dropdown inuser">
                            <a href="{{ account }}" title="{{ text_account }}" class="dropdown-toggle" data-toggle="dropdown">
                                <svg><use xlink:href="#huser"></use></svg><!-- <span>{{ text_login }}<span> --></a>
                            <ul class="dropdown-menu dropdown-menu-right haccount">
                                {% if logged %}
                                    <li><a href="{{ account }}"><i class="fa fa-user"></i>{{ text_account }}</a></li>
                                    <li><a href="{{ order }}"><i class="fa fa-history"></i>{{ text_order }}</a></li>
                                    <li><a href="{{ transaction }}"><i class="fa fa-exchange"></i>{{ text_transaction }}</a></li>
                                    <li><a href="{{ download }}"><i class="fa fa-download"></i>{{ text_download }}</a></li>
                                    <li><a href="{{ compare }}"><i class="fa fa-compress"></i>{{ text_comparepro }}</a></li>
                                    <li><a href="{{ logout }}"><i class="fa fa-sign-out"></i>{{ text_logout }}</a></li>
                                {% else %}
                                    <li><a href="{{ register }}"><i class="fa fa-user-plus"></i>{{ text_register }}</a></li>
                                    <li><a href="{{ login }}"><i class="fa fa-lock"></i>{{ text_login }}</a></li>
                                    <li><a href="{{ compare }}"><i class="fa fa-compress"></i>{{ text_comparepro }}</a></li>

                                {% endif %}
                            </ul>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
  </header>

  <div id="popupmenu" class="mfp-hide white-popup-block1">
      {{ popup }}
  </div>

  <div id="search" class="mfp-hide white-popup-block">
      {{ search }}
  </div>



